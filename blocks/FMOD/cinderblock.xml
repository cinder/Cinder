<?xml version="1.0" encoding="UTF-8" ?>
<cinder>
<block
	name="FMOD"
	id="org.libcinder.fmod"
	author="Cinder Project"
	license="GPL, Commercial"
	summary="FMOD library"
	version="4.44.06"
	>
	<includePath>include</includePath>
	<platform os="macosx">
		<dynamicLibrary>lib/macosx/libfmodex.dylib</dynamicLibrary>
		<buildCopy>lib/macosx/libfmodex.dylib</buildCopy>
	</platform>
	<platform os="msw">
		<staticLibrary>lib/msw/fmodex_vc.lib</staticLibrary>
		<buildCopy>lib/msw/fmodex.dll</buildCopy>
	</platform>
	<platform os="ios" sdk="device">
		<staticLibrary>lib/ios/libfmodex_iphoneos.a</staticLibrary>
	</platform>
	<platform os="ios" sdk="simulator">
		<staticLibrary>lib/ios/libfmodex_iphonesimulator.a</staticLibrary>
	</platform>
</block>
<template>templates/Basic FMOD/template.xml</template>
</cinder>